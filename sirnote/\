#include<fcntl.h>
#include<errno.h>
#include<assert.h>
int main(int argc,char *argv[])
{
	int fd,ret,n;
	fd = open("foo",0_RDONLY) 
	assert(fd > 0) ;
	while(1) {
		read(fd,buf,1);
		write(1,buf,1);
	}
}
